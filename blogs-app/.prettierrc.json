{
	"arrowParens": "always",
	"bracketSpacing": true,
	"endOfLine": "lf",
	"parser": "typescript",
	"quoteProps": "consistent",
	"semi": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"useTabs": true,
	"overrides": [
		{
			"files": "*.{ts,tsx}",
			"options": {
				"parser": "typescript"
			}
		},
		{
			"files": "*.css",
			"options": {
				"parser": "css"
			}
		}
	]
}
